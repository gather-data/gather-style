{
    "name": "gather-style",
    "version": "1.0.9",
    "main": "./dist/lib/index.js",
    "module": "./dist/lib/index.js",
    "types": "./dist/lib/index.d.ts",
    "files": ["dist"],
    "scripts": {
        "build": "tsc -p tsconfig.json && yarn build-assets",
        "build-assets": "cp -r src/assets dist/lib",
        "watch": "tsc -w -p tsconfig.json",
        "fix": "./node_modules/.bin/tslint -p tsconfig.json --fix",
        "jest": "./node_modules/.bin/jest -u",
        "prepublishOnly": "yarn build",
        "storybook":
            "ts-node ./node_modules/.bin/start-storybook -p 9004 -c .storybook",
        "test":
            "tslint -p tsconfig.json && yarn build && ./node_modules/.bin/jest --runInBand"
    },
    "author": "Ben Davis <bendavis381@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@storybook/react": "3.4.8",
        "@types/jest": "^22.0.0",
        "@types/node": "^10.0.3",
        "@types/react": "16.4.6",
        "babel-core": "6.26.3",
        "babel-eslint": "8.2.5",
        "eslint": "^3.19.0",
        "eslint-config-airbnb": "^15.0.2",
        "eslint-config-prettier": "^2.3.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-prettier": "2.1.2",
        "eslint-plugin-react": "^7.1.0",
        "prettier": "1.13.7"
    },
    "dependencies": {
        "@types/color": "3.0.0",
        "@types/hedron": "0.7.2",
        "@types/react-router-dom": "4.2.7",
        "color": "3.0.0",
        "hedron": "0.7.1",
        "hex-rgb": "3.0.0",
        "react-router-hash-link": "1.2.0",
        "react-scrollchor": "5.1.0",
        "styled-components": "3.3.3",
        "typescript": "2.9.2"
    },
    "peerDependencies": {
        "react": "16.4.1"
    }
}
